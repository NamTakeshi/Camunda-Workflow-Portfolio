    <bpmn:sendTask id="Activity_0a5d3hy" name="Bewerber zum Vorstellungsgespr√§ch einladen" camunda:type="external" camunda:topic="sendEmail">
      <bpmn:extensionElements>
        <camunda:executionListener event="start">
          <camunda:script scriptFormat="javascript">// Empf√§nger
var to = execution.getVariable("Email");

// Absender &amp; Betreff
var from = "hr@robocare.de";
var subject = "Einladung zum Vorstellungsgespr√§ch ‚Äì RoboCare";

// Termin
var interviewDatum = execution.getVariable("InterviewDatum");
var interviewUhrzeit = execution.getVariable("InterviewUhrzeit");

// Anrede
var anrede = execution.getVariable("Anrede");
var vorname = execution.getVariable("Vorname");
var nachname = execution.getVariable("Nachname");

var anredeText = "Sehr geehrte Damen und Herren";
if (anrede === "Herr") {
  anredeText = "Sehr geehrter Herr " + nachname;
} else if (anrede === "Frau") {
  anredeText = "Sehr geehrte Frau " + nachname;
}

// Freundlicher Mailtext
var mailText =
anredeText + ",\n\n" +
"vielen Dank f√ºr Ihre Bewerbung und Ihr Interesse an einer T√§tigkeit bei RoboCare.\n\n" +
"Wir haben Ihre Unterlagen gepr√ºft und freuen uns, Sie zu einem pers√∂nlichen Gespr√§ch einladen zu d√ºrfen.\n\n" +
"Der Gespr√§chstermin ist wie folgt geplant:\n" +
"Datum: " + interviewDatum + "\n" +
"Uhrzeit: " + interviewUhrzeit + "\n\n" +
"Das Gespr√§ch findet in unseren R√§umlichkeiten statt. Weitere Informationen erhalten Sie vor dem Termin.\n\n" +
"Wir freuen uns darauf, Sie kennenzulernen.\n\n" +
"Mit freundlichen Gr√º√üen\n\n" +
"Ihr RoboCare HR-Team";

// Mail-Variablen setzen
execution.setVariable("from", from);
execution.setVariable("to", to);
execution.setVariable("subject", subject);
execution.setVariable("mailText", mailText);</camunda:script>
        </camunda:executionListener>
