var os = connection.getOutputStream();
var writer = new java.io.OutputStreamWriter(os, "UTF-8");
writer.write(body);
writer.flush();
writer.close();
os.close();

var responseCode = connection.getResponseCode();
// Optional: Speichere den Code zur Kontrolle (200 = OK)
execution.setVariable("slack_response", responseCode);</bpmn:script>
