    <bpmn:sendTask id="Activity_08c0lkv" name="Angebot an Kunden senden" camunda:type="external" camunda:topic="sendEmail">
      <bpmn:extensionElements>
        <camunda:executionListener event="start">
          <camunda:script scriptFormat="javascript">var from = "service@robocare.de";
var to = execution.getVariable("Email");
var subject = "Ihr Angebot zu Ihrem Wartungs- / Reparaturauftrag";

// Kundendaten laden
var anrede = execution.getVariable("Anrede");
var vorname = execution.getVariable("Vorname");
var nachname = execution.getVariable("Nachname");
var maschinentyp = execution.getVariable("Maschinentyp");
var wartungstyp = execution.getVariable("Wartungstyp");
var kostenvoranschlag = execution.getVariable("Kostenvoranschlag");

// Fallback, falls keine Kosten gesetzt wurden
if (!kostenvoranschlag) {
  kostenvoranschlag = "wird separat übermittelt";
}

// Mailtext bauen
var mailText =
  "Sehr geehrte/r " + anrede + " " + nachname + ",\n\n" +
  "vielen Dank für Ihren Auftrag bei RoboCare.\n" +
  "Wir haben Ihre Maschine (" + maschinentyp + ") geprüft und freuen uns, " +
  "Ihnen nun ein Angebot für den folgenden Service unterbreiten zu können:\n\n" +
  "Auftragsart: " + wartungstyp + "\n" +
  "Kostenvoranschlag: " + kostenvoranschlag + " EUR\n\n" +
  "Sollten Sie Rückfragen haben oder Änderungen wünschen,\n" +
  "melden Sie sich gerne bei uns.\n\n" +
  "Mit freundlichen Grüßen\n" +
  "Ihr RoboCare Service-Team";

execution.setVariable("from", from);
execution.setVariable("subject", subject);
execution.setVariable("to", to);
execution.setVariable("mailText", mailText);</camunda:script>
        </camunda:executionListener>
      </bpmn:extensionElements>
